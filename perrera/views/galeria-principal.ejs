<%- include('comunes/header',{titulo:'titulo'}) %>

<body>
<div class="container">


    <div class="row">
        <div class = col-sm-2>

        </div>
        <div class = col-sm-8>
            <h1 align="center"> Bienvenido a la Perrera Cuasqui </h1>
        </div>
        <div class = col-sm-2>

        </div>

    </div>



    <div class="row">
        <div class ="col-sm-2"></div>
        <div class = "col-sm-8">


            <div id="carouselExampleInterval" class="carousel slide" data-ride="carousel">
                <div class="carousel-inner">
                    <div class="carousel-item active" data-interval="10000">
                        <img src="data:image/jpeg;base64," width="100" height="300" class="d-block w-100" alt="...">
                    </div>
                    <div class="carousel-item" data-interval="2000">
                        <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQhB82XwnxSmPIer3V_bStkR-RqxlhFFcIc7GKoxX7NUXHt0N0S" width="100" height="300"class="d-block w-100" alt="...">
                    </div>
                    <div class="carousel-item">
                        <img src="https://www.rionegro.com.ar/documents/1/0/650x366/0c0/0d0/none/12350/BIXD/image_content_9535595_20180406133200.jpg" width="100" height="300" class="d-block w-100" alt="...">
                    </div>
                </div>
                <a class="carousel-control-prev" href="#carouselExampleInterval" role="button" data-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                </a>
                <a class="carousel-control-next" href="#carouselExampleInterval" role="button" data-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                </a>
            </div>

        </div>
        <div class ="col-sm-2"></div>
    </div>
    <br>
    <p>Buscar Perro por nombre o raza</p>

    <script>
        function borrar(evento) {
            console.log(evento)
            const respuesta = confirm('¿Estás seguro?')
            if (!respuesta) {
                evento.preventDefault()
            }
            console.log(respuesta)
        }
    </script>

    <form action="/Perro/inicio" method="get">
        <div class="row">
            <div class="col-sm-9">
                <div class="form-group">
                    <label for="busqueda">Busqueda</label>
                    <input class="form-control" id="busqueda" type="text" name="busqueda">
                </div>
            </div>
            <div class="col-sm-3">
                <button type="submit"
                        class="btn btn-outline-info btn-buscar btn-block">Buscar
                </button>

            </div>
        </div>
    </form>

    <% const existeMensaje = typeof mensaje !== "undefined" ? true : false; %>

    <% if(existeMensaje){ %>

        <div class="animated fadeOut alert alert-<%= accion %>" role="alert">
            MENSAJE: <%= mensaje %>
        </div>

    <% } %>



















    <h2>Perros Disponibles</h2>

    <table class="table table-hover animated fadeInUp">
        <caption>Lista de perros</caption>
        <!--tr -> table row-->
        <thead>
        <tr>
            <!--th = table header-->
            <th width="25%">Nombre del perro</th>
            <th width="25%">Raza</th>
            <th width="20%">Tamaño</th>
            <th width="30%">Acciones</th>
        </tr>
        </thead>

        <% arreglo.forEach ((perro) => { %>
            <tr>
                <td>
                    <%= perro.nombrePerro %>
                </td>
                <td>
                    <%= perro.razaPerro %>
                </td>
                <td>
                    <%= perro.tamanioPerro %>
                </td>
                <td>


                    </form>

                        <button type="button"
                                class="btn btn-outline-info btn-block">ADOPTAR
                        </button>

                </td>
            </tr>
        <% }) %>

    </table>
</div>
<%- include('comunes/librerias') %>
</body>

</html>

